%section#editor
%section.list
  %nav
    = link_to "Incomplete", "#", class: 'active'
    = link_to "Complete"
    = link_to "All"
  %ul.tasks
    - 50.times do
      %li
        .complete= link_to("complete")
        .title User signs in
        .delete= link_to("delete")
%section.messages
  .form_container
    %form
      %textarea(placeholder="Message...")
      .actions
        = link_to("attach")
        %input(type="submit" value="Add message")
